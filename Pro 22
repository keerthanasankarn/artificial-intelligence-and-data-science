% Database of birds and whether they can fly
bird(canary, true).
bird(ostrich, false).
bird(penguin, false).
bird(sparrow, true).
bird(eagle, true).

% Query to check if a bird can fly
can_fly(Bird) :-
    bird(Bird, true),
    write(Bird),
    write(' can fly.'),
    nl.
can_fly(Bird) :-
    bird(Bird, false),
    write(Bird),
    write(' cannot fly.'),
    nl.

